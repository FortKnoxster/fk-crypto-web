{
  "name": "fkx-crypto-web",
  "version": "1.0.0",
  "description": "FortKnoxsters encryption algorithms used in the web app.",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "nyc ava --verbose",
    "test:watch": "nyc ava --verbose --watch"
  },
  "lint-staged": {
    "src/**/*.js": "eslint"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill",
      "./src/test/helpers/_setup-browser-env.js"
    ]
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "FortKnoxster",
  "license": "ISC",
  "devDependencies": {
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "ava": "^2.4.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-register": "^6.26.0",
    "browser-env": "^3.2.6",
    "eslint": "^6.4.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "node-webcrypto-ossl": "^1.0.48",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
